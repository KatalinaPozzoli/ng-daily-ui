{
  "version": 1,
  "projects": {
    "portfolio": {
      "projectType": "application",
      "root": "apps/portfolio",
      "sourceRoot": "apps/portfolio/src",
      "prefix": "katita",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/portfolio",
            "index": "apps/portfolio/src/index.html",
            "main": "apps/portfolio/src/main.ts",
            "polyfills": "apps/portfolio/src/polyfills.ts",
            "tsConfig": "apps/portfolio/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/portfolio/src/favicon.ico",
              "apps/portfolio/src/assets"
            ],
            "styles": ["apps/portfolio/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/portfolio/src/environments/environment.ts",
                  "with": "apps/portfolio/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "portfolio:build:production"
            },
            "development": {
              "browserTarget": "portfolio:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "portfolio:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/portfolio/src/**/*.ts",
              "apps/portfolio/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/portfolio"],
          "options": {
            "jestConfig": "apps/portfolio/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "portfolio-e2e": {
      "root": "apps/portfolio-e2e",
      "sourceRoot": "apps/portfolio-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/portfolio-e2e/cypress.json",
            "devServerTarget": "portfolio:serve:development",
            "tsConfig": "apps/portfolio-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "portfolio:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/portfolio-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["portfolio"]
    },
    "sandbox": {
      "projectType": "application",
      "root": "apps/sandbox",
      "sourceRoot": "apps/sandbox/src",
      "prefix": "katita",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/sandbox",
            "index": "apps/sandbox/src/index.html",
            "main": "apps/sandbox/src/main.ts",
            "polyfills": "apps/sandbox/src/polyfills.ts",
            "tsConfig": "apps/sandbox/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/sandbox/src/favicon.ico",
              "apps/sandbox/src/assets",
              {
                "glob": "**/*",
                "input": "libs/ui-components/src/lib/simplify-sign-in/assets",
                "output": "assets/simplify"
              }
            ],
            "styles": ["apps/sandbox/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/sandbox/src/environments/environment.ts",
                  "with": "apps/sandbox/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "sandbox:build:production"
            },
            "development": {
              "browserTarget": "sandbox:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sandbox:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/sandbox/src/**/*.ts",
              "apps/sandbox/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/sandbox"],
          "options": {
            "jestConfig": "apps/sandbox/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "ui-components": {
      "projectType": "library",
      "root": "libs/ui-components",
      "sourceRoot": "libs/ui-components/src",
      "prefix": "katita",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui-components"],
          "options": {
            "jestConfig": "libs/ui-components/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui-components/src/**/*.ts",
              "libs/ui-components/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    }
  }
}
